{
  "scopeName": "source.boxdsl",
  "patterns": [
    {
      "name": "keyword.control.boxdsl",
      "match": "\\b(plugin|define|tag|attr|content|selfclosing|name|author|version|description)\\b"
    },
    {
      "name": "support.function.boxdsl",
      "match": "\\b[A-Z][A-Za-z0-9_]*\\b"
    },
    {
      "name": "variable.parameter.boxdsl",
      "match": "\\b[a-z][A-Za-z0-9_]*\\b"
    },
    {
      "name": "string.quoted.double.boxdsl",
      "match": "\".*?\""
    },
    {
      "name": "punctuation.definition.boxdsl",
      "match": "[();>]"
    }
  ]
}
